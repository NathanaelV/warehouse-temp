<h1>Galpão <%= @warehouse.code %></h1>
<h2><%= @warehouse.description %></h2>

<div>
  <dl>
    <div>
      <dt>Nome: </dt>
      <dd><%= @warehouse.name %></dd>
    </div>

    <div>
      <dt>Cidade: </dt>
      <dd><%= @warehouse.city %></dd>
    </div>

    <div>
      <dt>Área: </dt>
      <dd><%= @warehouse.area %> m²</dd>
    </div>

    <div>
      <dt>Endereço: </dt>
      <dd><%= @warehouse.address %> CEP: <%= @warehouse.cep %></dd>
    </div>
  </dl>
</div>
